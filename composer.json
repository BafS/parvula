{
	"name": "bafs/parvula",
	"description": "Simple, Markdown and Fast CMS",
	"type": "project",
	"keywords": ["cms", "markdown"],
	"license": "MIT",
	"homepage": "https://github.com/BafS/parvula",
	"authors": [
		{
			"name": "BafS",
			"email": "fabacrans@gmail.com"
		}
	],
	"support": {
		"issues": "https://github.com/BafS/parvula/issues",
		"source": "https://github.com/BafS/parvula"
	},
	"minimum-stability": "stable",
	"require": {
		"php": ">=5.4",
		"league/plates": "3.*",
		"erusev/parsedown-extra": "^0.7.0",
